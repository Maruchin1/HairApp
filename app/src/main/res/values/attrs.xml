<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="title" format="string" />
    <attr name="icon" format="reference" />

    <declare-styleable name="ToolbarView">
        <attr name="title" />
        <attr name="toolbarMode" format="enum">
            <enum name="home" value="0" />
            <enum name="standard" value="1" />
        </attr>
        <attr name="optionsMenu" format="reference" />
    </declare-styleable>

    <declare-styleable name="CareProductEditSectionView">
        <attr name="title" />
    </declare-styleable>

    <declare-styleable name="ProductsProportionView" />

    <declare-styleable name="ProductApplicationsView">
        <attr name="itemsSelectable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="NoContentView">
        <attr name="icon" />
        <attr name="title" />
        <attr name="useArrow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SectionCardView">
        <attr name="icon" />
        <attr name="titleResId" format="reference" />
    </declare-styleable>

    <declare-styleable name="StatCounterView">
        <attr name="value" format="string" />
        <attr name="label" format="string" />
    </declare-styleable>

</resources>