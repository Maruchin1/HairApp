<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>

        <variable
            name="controller"
            type="com.example.care_schema_details.CareSchemaDetailsActivity" />

        <variable
            name="viewModel"
            type="com.example.care_schema_details.components.CareSchemaDetailsViewModel" />
    </data>

    <androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".CareSchemaDetailsActivity">

        <include
            android:id="@+id/appbar"
            layout="@layout/view_care_schema_details_appbar"
            app:controller="@{controller}"
            app:viewModel="@{viewModel}" />

    </androidx.coordinatorlayout.widget.CoordinatorLayout>
</layout>